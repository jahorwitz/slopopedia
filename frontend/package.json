{
  "name": "slopopedia-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "npx prettier --check --ignore-unknown \"src/**/*.{js,jsx}\"",
    "lint:fix": "npx prettier --write --ignore-unknown \"src/**/*.{js,jsx}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "storybook:node16": "start-storybook -p 6006",
    "storybook:node18:windows": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "**/*.js": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "framer-motion": "^8.5.5",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "styled-components": "^5.3.6",
    "vite-plugin-babel-macros": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/blocks": "^7.0.22",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.15",
    "@storybook/react-vite": "^7.0.22",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "git-commit-msg-linter": "^4.5.0",
    "husky": "^8.0.0",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.22",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.0.0",
    "vitest": "^0.28.4"
  }
}
